{
  "name": "Better Solarized Dark Italics",
  "semanticHighlighting": true,
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "activeGuide": "#FFFFFF55",
        "background": "#002B36",
        "foreground": "#839496",
        "gutter": "#002B36",
        "popupCss": "\nhtml {\n  background-color: #123a44;\n  color: #839496;\n}\n\na {\n  color: #268BD2;\n}\n\n.error,\n.deleted {\n  color: #DC322F;\n}\n\n.success,\n.inserted,\n.name {\n  color: #859900;\n}\n\n.warning,\n.modified {\n  color: #B58900;\n}\n\n.type {\n  color: #2AA198;\n  font-style: italic;\n}\n\n.param {\n  color: #CB4B16;\n}\n\n.current {\n  text-decoration: underline;\n}\n",
        "selectionBorder": "#FFFFFF16",
        "stackGuide": "#FFFFFF40"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#586E75"
      }
    },
    {
      "name": "Jekyll Tags",
      "scope": [
        "punctuation.output.jekyll",
        "punctuation.tag.jekyll",
        "punctuation.frontmatter.jekyll",
        "punctuation.output.liquid",
        "punctuation.tag.liquid"
      ],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Jekyll Operators",
      "scope": [
        "entity.name.tag.jekyll",
        "entity.name.tag.liquid"
      ],
      "settings": {
        "foreground": "#D33682",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Jekyll Variable",
      "scope": [
        "punctuation.tag.jekyll",
        "punctuation.tag.liquid"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Jekyll Content",
      "scope": [
        "variable.other.jekyll",
        "variable.other.liquid"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Jekyll arguments",
      "scope": [
        "support.function.with-args.jekyll"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Javascript JSDoc Symbol",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#839496",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#DC322F",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#6C71C4",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#6C71C4"
      }
    },
    {
      "name": "CSS/Scss At-Rules",
      "scope": "keyword.control, keyword.operator.new, keyword.operator.module, variable.language.super",
      "settings": {
        "foreground": "#6C71C4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C# Keyword",
