{
  "name": "ginfuru-better-solarized-dark-theme",
  "displayName": "Better Solarized",
  "description": "A Better Solarized theme for Visual Studio Code include light and dark versions",
  "publisher": "ginfuru",
  "version": "0.9.10",
  "engines": {
    "vscode": "^1.13.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite-node src/index.ts -w",
    "build": "vite-node src/index.ts",
    "build:ext": "vsce package --out ./releases/%npm_package_version%.vsix --no-yarn",
    "fix": "prettier --write ./src/**/*.ts && eslint src/**/*.{vue,ts} --fix",
    "release": "gh release create v%npm_package_version% --notes-file ./releases/%npm_package_version%.md --generate-notes"
  },
  "contributes": {
    "themes": [
      {
        "label": "Better Solarized Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Better Solarized Dark-color-theme.json"
      },
      {
        "label": "Better Solarized Dark Italics",
        "uiTheme": "vs-dark",
        "path": "./themes/Better Solarized Dark Italic-color-theme.json"
      },
      {
        "label": "Better Solarized Light",
        "uiTheme": "vs",
        "path": "./themes/Better Solarized Light-color-theme.json"
      }
    ]
  },
  "categories": [
    "Themes"
  ],
  "icon": "assets/images/icon.png",
  "galleryBanner": {
    "color": "#00212b",
    "theme": "dark"
  },
  "keywords": [
    "atom",
    "better solarized",
    "better",
    "dark",
    "light",
    "solarized dark",
    "solarized light",
    "solarized",
    "sublime",
    "theme"
  ],
  "license": "SEE license inf license.txt",
  "homepage": "https://github.com/ginfuru/vscode-better-solarized",
  "repository": {
    "type": "git",
    "url": "https://github.com/ginfuru/vscode-better-solarized.git"
  },
  "bugs": {
    "url": "https://github.com/ginfuru/vscode-better-solarized/issues",
    "email": "402910+edheltzel@users.noreply.github.com"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/edheltzel"
  },
  "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589",
  "dependencies": {
    "@types/node": "^20.12.2",
    "@types/vscode": "^1.87.0",
    "@vscode/vsce": "^2.24.0",
    "biome": "^0.3.3",
    "colord": "^2.9.3",
    "ovsx": "^0.9.0",
    "prettier": "^3.2.5",
    "typerscript": "^0.2.3",
    "vite-node": "^1.4.0"
  }
}
